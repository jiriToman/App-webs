{
  "name": "tappywebs",
  "version": "1.0.0",
  "description": "multipage web pro Tappytaps",
  "main": "index.js",
  "scripts": {
    "start-test-bibino": "cross-env NODE_ENV=test APP_NAME=bibino node index.js",
    "start-prod-bibino": "cross-env NODE_ENV=production APP_NAME=bibino node index.js",
    "start-test-barkio": "cross-env NODE_ENV=test APP_NAME=barkio node index.js",
    "start-prod-barkio": "cross-env NODE_ENV=production APP_NAME=barkio node index.js",
    "genStr": "stringsgen gen",
    "upStr": "stringsgen up",
    "downStr": "stringsgen down",
    "strings-bibino-down": "stringsgen down --config ./stringsgenrc-bibino",
    "strings-barkio-down": "stringsgen down --config ./stringsgenrc-barkio",
    "formStr": "node JsonToNested",
    "strings": "sudo stringsgen gen && sudo stringsgen up && sudo stringsgen down && node JsonToNested",
    "stringsB": "sudo stringsgen gen && sudo stringsgen up && sudo stringsgen down && node JsonToNested bar",
    "demon": "npm nodemon -e js,json,hbs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiriToman/App-webs.git"
  },
  "keywords": [],
  "author": "Jiri Toman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiriToman/App-webs/issues"
  },
  "homepage": "https://github.com/jiriToman/App-webs#readme",
  "dependencies": {
    "@babel/core": "^7.14.0",
    "@tappytaps/translation-tool": "^1.3.15",
    "bootstrap": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "flat": "^5.0.2",
    "geoip-lite": "^1.4.2",
    "got": "^11.8.2",
    "hbs": "^4.1.2",
    "i18next": "^15.1.3",
    "i18next-express-middleware": "^1.9.1",
    "i18next-http-middleware": "^3.1.1",
    "i18next-node-fs-backend": "^2.1.3",
    "mobile-detect": "^1.4.5",
    "node-cache": "^5.1.2",
    "parse": "^2.19.0",
    "require-all": "^3.0.0",
    "serve-static": "^1.14.1",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0"
  }
}

<meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <meta name="robots" content="noindex,noarchive,nofollow">
  <!-- Primary Meta Tags -->
  <meta name="description" content="{{t "home.description" }}">
  <title>{{t "home.title" }}</title>
  <meta name="title" content="{{t "home.title" }}">

  <link rel="canonical" href="{{t "home.canonical" }}" />


    {{> head/analyticsBibino}}


  <!-- Open Graph / Facebook -->
  <meta property="og:site_name" content="{{t "home.title" }}">
  <meta property="og:type" content="website">
  <meta property="og:url" content="{{t "home.ogurl" }}">
  <meta property="og:title" content="{{t "home.title" }}">
  <meta property="og:description" content="{{t "home.description" }}">
  <meta property="og:image" content="{{t "home.og" }}">
  <meta property="og:locale" content="{{t "home.locale" }}">
  <meta property="og:locale:alternate" content="{{t "home.alternate1" }}">
  <meta property="og:locale:alternate" content="{{t "home.alternate2" }}">
  <!-- Twitter -->
  <meta property="twitter:card" content="summary_large_image">
  <meta property="twitter:url" content="https://barkio.com/">
  <meta property="twitter:title" content="{{t "home.title" }}">
  <meta property="twitter:description" content="{{t "home.description" }}">
  <meta property="twitter:image" content="{{t "home.og" }}">
  <!-- Favicon -->
  {{!-- <link rel="icon" href="/favicon.ico?v=1.0"/> --}}
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
